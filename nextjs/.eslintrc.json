{
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json",
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "typeorm"
  ],
  "rules": {
    // "typeorm/rule-name": 2
  },
  "ignorePatterns": ["**/*.js", "./db/entity/Listing.ts"]
}